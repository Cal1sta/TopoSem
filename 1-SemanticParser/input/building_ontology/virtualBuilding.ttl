# -- PREFIXES --
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix brick: <https://brickschema.org/schema/1.4/Brick#> .
@prefix bldg: <http://example.com/myVirtualBuilding#> .

# -- BUILDING AND SPATIAL STRUCTURE (UNCHANGED) --
bldg:MySmartBuilding a brick:Building ;
    rdfs:label "My Smart Building" ;
    brick:hasPart bldg:Floor_1,
                  bldg:Floor_2,
                  bldg:Floor_3,
                  bldg:Roof . 

# -- FLOOR 1 AND SPACES --
bldg:Floor_1 a brick:Floor ;
    rdfs:label "First Floor" ;
    brick:isPartOf bldg:MySmartBuilding ;
    brick:hasPart bldg:Main_Entrance,
                  bldg:Lobby_1,
                  bldg:Hallway_1,
                  bldg:Office_1A,
                  bldg:Office_1B,
                  bldg:ConfRoom_1,
                  bldg:Server_Room,
                  bldg:Control_Room,
                  bldg:Mechanical_Room_1 . 

bldg:Main_Entrance a brick:Entrance ; rdfs:label "Main Entrance" ; brick:isPartOf bldg:Floor_1 .
bldg:Lobby_1 a brick:Lobby ; rdfs:label "Main Lobby - Floor 1" ; brick:isPartOf bldg:Floor_1 .
bldg:Hallway_1 a brick:Hallway ; rdfs:label "Hallway - Floor 1" ; brick:isPartOf bldg:Floor_1 .
bldg:Office_1A a brick:Office ; rdfs:label "Office 1A" ; brick:isPartOf bldg:Floor_1 .
bldg:Office_1B a brick:Office ; rdfs:label "Office 1B" ; brick:isPartOf bldg:Floor_1 .
bldg:ConfRoom_1 a brick:Conference_Room ; rdfs:label "Conference Room 1" ; brick:isPartOf bldg:Floor_1 .
bldg:Server_Room a brick:Server_Room ; rdfs:label "Server Room" ; brick:isPartOf bldg:Floor_1 .
bldg:Control_Room a brick:Control_Room ; rdfs:label "Control Room" ; brick:isPartOf bldg:Floor_1 .
bldg:Mechanical_Room_1 a brick:Mechanical_Room ; rdfs:label "Mechanical Room - Floor 1" ; brick:isPartOf bldg:Floor_1 .

# -- FLOOR 2 AND SPACES --
bldg:Floor_2 a brick:Floor ; rdfs:label "Second Floor" ; brick:isPartOf bldg:MySmartBuilding ;
    brick:hasPart bldg:Hallway_2, bldg:Office_2A, bldg:Office_2B, bldg:Office_2C, bldg:ConfRoom_2 .
bldg:Hallway_2 a brick:Hallway ; rdfs:label "Hallway - Floor 2" ; brick:isPartOf bldg:Floor_2 .
bldg:Office_2A a brick:Office ; rdfs:label "Office 2A" ; brick:isPartOf bldg:Floor_2 .
bldg:Office_2B a brick:Office ; rdfs:label "Office 2B" ; brick:isPartOf bldg:Floor_2 .
bldg:Office_2C a brick:Office ; rdfs:label "Office 2C" ; brick:isPartOf bldg:Floor_2 .
bldg:ConfRoom_2 a brick:Conference_Room ; rdfs:label "Conference Room 2" ; brick:isPartOf bldg:Floor_2 .

# -- FLOOR 3 AND SPACES --
bldg:Floor_3 a brick:Floor ; rdfs:label "Third Floor" ; brick:isPartOf bldg:MySmartBuilding ;
    brick:hasPart bldg:Hallway_3, bldg:Office_3A, bldg:Office_3B, bldg:ConfRoom_3 .
bldg:Hallway_3 a brick:Hallway ; rdfs:label "Hallway - Floor 3" ; brick:isPartOf bldg:Floor_3 .
bldg:Office_3A a brick:Office ; rdfs:label "Office 3A" ; brick:isPartOf bldg:Floor_3 .
bldg:Office_3B a brick:Office ; rdfs:label "Office 3B" ; brick:isPartOf bldg:Floor_3 .
bldg:ConfRoom_3 a brick:Conference_Room ; rdfs:label "Conference Room 3" ; brick:isPartOf bldg:Floor_3 .

# -- ROOF DEFINITION --
bldg:Roof a brick:Rooftop ; rdfs:label "Roof" ; brick:isPartOf bldg:MySmartBuilding .


# =======================================================
# == HVAC EQUIPMENT ADDITIONS START HERE ==
# =======================================================

# -- CENTRAL HVAC EQUIPMENT (REVISED AND ENHANCED) --
# These devices serve the entire building.
bldg:Central_AHU a brick:AHU ;
    rdfs:label "Central Air Handler Unit" ;
    brick:isFedBy bldg:Main_Chiller, bldg:Main_Boiler . 
bldg:Mechanical_Room_1 brick:isLocationOf bldg:Central_AHU .

bldg:Main_Chiller a brick:Chiller ; 
    rdfs:label "Main Chiller" ;
    brick:isFedBy bldg:Main_CoolingTower . 
bldg:Mechanical_Room_1 brick:isLocationOf bldg:Main_Chiller .

bldg:Main_Boiler a brick:Boiler ; rdfs:label "Main Boiler" .
bldg:Mechanical_Room_1 brick:isLocationOf bldg:Main_Boiler .

bldg:Main_CoolingTower a brick:Cooling_Tower ; rdfs:label "Main Cooling Tower" .
bldg:Roof brick:isLocationOf bldg:Main_CoolingTower .

# Outdoor environment sensors associated with the AHU
bldg:OAT_Sensor a brick:Outside_Air_Temperature_Sensor ; rdfs:label "Outside Air Temperature Sensor" .
bldg:OARH_Sensor a brick:Outside_Air_Relative_Humidity_Sensor ; rdfs:label "Outside Air Relative Humidity Sensor" .
bldg:Central_AHU brick:hasPoint bldg:OAT_Sensor, bldg:OARH_Sensor .
bldg:MySmartBuilding brick:isLocationOf bldg:OAT_Sensor, bldg:OARH_Sensor .


# -- ZONAL HVAC EQUIPMENT --

# -- Floor 1 Equipment --
# Office 1A
bldg:VAV_1A a brick:VAV ; rdfs:label "VAV for Office 1A" ; brick:isFedBy bldg:Central_AHU ; brick:feeds bldg:Office_1A .
bldg:TempSensor_1A a brick:Room_Air_Temperature_Sensor ; rdfs:label "Temperature Sensor for Office 1A" .
bldg:Office_1A brick:isLocationOf bldg:VAV_1A, bldg:TempSensor_1A .
bldg:VAV_1A brick:hasPoint bldg:TempSensor_1A .

# Office 1B (similar setup)
